		function gotogzlynew(search){
			var url = "http://gzlynew.mofcom.gov.cn/gzlynew/servlet/SearchServlet?OP=searchText&searchText=";
			var enc = encodeURI(search);
			var url = url + enc;
			window.open(url);
		}
