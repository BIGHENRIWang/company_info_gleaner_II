<script type="text/javascript">
$(function(){
	    
		
		var $div_li2 =$(".wmswTab1 ul li");
		var $div_li3 =$(".ywzttab ul li");
                var $div_li4 =$("#newsTitle h4 a");
                var $div_li5 =$(".navTitle1 ul li.zxbsTab");
	   
	  
		
		$div_li2.click(function(){
			$(this).addClass("cur")         
				   .siblings("").removeClass("cur"); 
            var index2 =  $div_li2.index(this);  
			$(".wmswF")   	
					.eq(index2).show()  
					.siblings().hide();
		})
		
		$div_li3.hover(function(){
			$(this).addClass("cur")         
				   .siblings("").removeClass("cur"); 
            var index3 =  $div_li3.index(this);  
			$(".ywzt-div a")   	
					.eq(index3).show()  
					.siblings().hide();
		})


                $div_li4.hover(function(){
			$(this).addClass("cur")         
				   .siblings("").removeClass("cur"); 
            var index4 =  $div_li4.index(this);  
			$("#newsList div")   	
					.eq(index4).show()  
					.siblings().hide();
		})

                $div_li5.hover(function(){
			$(this).addClass("cur")         
				   .siblings("li").removeClass("cur"); 
                var index5 =  $div_li5.index(this);  
			$(".zxbsZcon div.zxbsCon")   	
					.eq(index5).show()  
					.siblings().hide();
		})
		
		

var page = new domUpDown('.tabCon_03','.u_btn',104);
		

})
