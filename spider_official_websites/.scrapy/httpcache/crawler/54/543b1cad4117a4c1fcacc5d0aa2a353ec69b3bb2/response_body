    <script language="javascript" type="text/javascript">
       function doInter(v){
       
          var ao2= document.getElementById('stateother');
        if(v=='OTHER'){
          ao2.style.visibility ='visible';
          ao2.disabled=false;
          ao2.style.display='block';
					ao2.focus();
        }else{
          ao2.value='';
          ao2.style.visibility='hidden';
          ao2.style.display='none';
        }
      
      } 
    
      function other(v){
       
          var ao2= document.getElementById('airlineother');
        if(v=='OTHER' || v=='AIRAMBU'){
            ao2.style.visibility ='visible';
          ao2.disabled=false;
          ao2.style.display='block';
        }else{
          ao2.value='';
          ao2.style.visibility='hidden';
          ao2.style.display='none';
        }
      
      }	
    function showorhide(){
      
      var show = 'block';
      var hide = 'none';
      var a = document.getElementById('aaname');
      var r = document.getElementById('rrname');
      
      if(document.myform.consumertype[document.myform.consumertype.selectedIndex].value == "AA")
      {
        a.style.display=show;
        r.style.display=hide	;	
      }
      else if(document.myform.consumertype[document.myform.consumertype.selectedIndex].value == "RR"||document.myform.consumertype[document.myform.consumertype.selectedIndex].value == "OO"||document.myform.consumertype[document.myform.consumertype.selectedIndex].value == "LL"||document.myform.consumertype[document.myform.consumertype.selectedIndex].value == "GG")
      {
        a.style.display=show;
        r.style.display=show;
      }	
      else
      {
        a.style.display=hide;
        r.style.display=hide;
        alert("Please select who you are");
      }
    }	   
    
    function chk(){
     if(document.getElementById("fileDocument").value=="")
     {
      alert("Please click the [Browse] button to select the file you wish to upload");
       return false; 
     }
    
    }
    
    function getFilenameFromPath(_path) {
      // extract the filename from the entire path
      // test if pc or mac
      var tmpPath = _path.substring(3, _path.length);
      if (tmpPath.indexOf("\\")) {
        var arrFile = tmpPath.split("\\");
      } else {
        var arrFile = tmpPath.split("/");
      }
      return arrFile[arrFile.length-1];
    }	
    
    function getFilename(_filename) {
      // extract the filename from the entire path
      // test if pc or mac
      if (_filename.indexOf("\\")) {
        var arrFile = _filename.split("\\");
      } else {
        var arrFile = _filename.split("/");
      }
      return arrFile[arrFile.length-1];
    } 
                                  
