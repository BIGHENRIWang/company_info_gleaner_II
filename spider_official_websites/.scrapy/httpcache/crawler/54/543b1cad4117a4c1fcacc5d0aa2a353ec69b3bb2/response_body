		<script type="text/javascript">
      loadSubmit = function() {
				document.getElementById("SubmitESForm").value = "Please Wait!";
				setTimeout(function(){
					document.getElementById("SubmitESForm").disabled = true;
				},1);
				document.getElementById("progress").style.visibility = "visible";
        ProgressImage = document.getElementById('progress_image');		
        setTimeout("ProgressImage.src = ProgressImage.src",6000);
        return true;
      }
			function chkLen() {
				var field1 = document.getElementById('compl_desc');
				var field2 = document.getElementById('counterSpc');
				var maxlimit = 3000;
				var remChars = maxlimit - field1.value.length ;
				if (remChars < 0) {field1.value = field1.value.substring( 0, maxlimit );}
				remChars = maxlimit - field1.value.length ;
				field2.innerHTML = '(Chars left: ' + remChars + ')';
				return true;
			}
    </script>
