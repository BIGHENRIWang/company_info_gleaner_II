				<script type="text/javascript">
          window.onload = function() {
						setTimeout(function(){
							var myElem = document.getElementById("uploadedFiles");
							if (myElem)
								myElem.focus();
							}, 1000);
							if(document.getElementById("ErrMsgDataInsert") && document.getElementById("ErrMsgDataInsert").innerHTML != '') {
								document.getElementById("ErrMsgDataInsert").focus();
							}
							if(document.getElementById("ErrMsgDataType") && document.getElementById("ErrMsgDataType").innerHTML != '') {
								document.getElementById("ErrMsgDataType").focus();
							}
							if(document.getElementById("ErrMsgReq") && document.getElementById("ErrMsgReq").innerHTML != '') {
								document.getElementById("ErrMsgReq").focus();
							}
							if(document.getElementById("errorMsg") && document.getElementById("errorMsg").innerHTML != '') {
								document.getElementById("errorMsg").focus();
							}
          };
