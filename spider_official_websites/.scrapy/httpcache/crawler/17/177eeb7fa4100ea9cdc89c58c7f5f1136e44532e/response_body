 <script type="text/javascript">
   $(function() {
	    var name = $( "#troubleShootUserName"),
	            contact = $( "#troubleShootUserContact"),
	            issue = $( "#issueText");
	    var allFields = $( [] ).add( name ).add( contact ).add( issue );
	    var troubleShootingDialog = $("#dialogReportIssue").dialog({
	        autoOpen: false,
	        modal: true,
	        height: 'auto',
	        width: '80%',
	        draggable: true,
	        buttons: {
	            "Send": sendTroubleShootingInformation,
	            Cancel: function() {
	            	resetDialog($( this ));
	            }
	        }
	    });
	    $("#reportIssue").on("click", function() {
	        troubleShootingDialog.dialog("open");
	    });

	    function sendTroubleShootingInformation() {
	        var valid = true;
	        valid = valid && validateUserName();
	        valid = valid && validateContactInfo();
	        valid = valid && validateIssueDescription();

	        if ( valid ) {
	            var ajaxResponse = $.ajax({
	                type: 'POST',
	                url: '/public/home/issue',
	                contentType : "application/json",
	                dataType : 'json',
	                data: JSON.stringify({'userName':name.val(),
	                    'userContactInfo':contact.val(),
	                    'issueText':issue.val()})
	            });
	            resetDialog(troubleShootingDialog);
	        }
	        return valid;
	    }

	    var form = troubleShootingDialog.find( "form" ).on( "submit", function( event ) {
	        event.preventDefault();
	        sendTroubleShootingInformation();
	    });

	    function resetDialog(d) {
	    	form[ 0 ].reset();
            //allFields.removeClass( "ui-state-error" );
            //allFields.val('');
            //$( "#validateTips" ).text('');
            d.dialog( "close" );
	    }
	});

    var validationRegex = /^([0-9a-z_\s&$#@\-,:\.;'()/])+$/i;
    
	function validateUserName() {
	    var name = $( "#troubleShootUserName")
	    var valid = true;
	    valid = valid && checkLength( name, "your name", 1, 100 );
	    valid = valid && checkRegexp( name, validationRegex, "Your name consists invalid character(s)" );
	    handleValidCondition(valid, name);
	    return valid;
	}

	function validateContactInfo() {
	    var valid = true;
	    var contact = $( "#troubleShootUserContact");
	    valid = valid && checkLength( contact, "your contact information", 1, 100 );
	    valid = valid && checkRegexp( contact, validationRegex, "Your contact information consists invalid character(s)" );
	    handleValidCondition(valid, contact);
	    return valid;
	}

	function validateIssueDescription() {
	    var valid = true;
	    var issue = $( "#issueText");
	    valid = valid && checkLength( issue, "issue description", 1, 1000 );
	    valid = valid && checkRegexp( issue, validationRegex, "The issue description consists invalid character(s)" );
	    handleValidCondition(valid, issue);
	    return valid;
	}

	function handleValidCondition(valid, o) {
	    if(valid) {
	        o.removeClass( "ui-state-error" );
	        $( "#validateTips" ).text('');
	    }
	}

	function checkLength( o, n, min, max ) {
	    if ( o.val().length > max || o.val().length < min ) {
	        o.addClass( "ui-state-error" );
	        updateTips( "Length of " + n + " must be between " +
	                min + " and " + max + "." );
	        return false;
	    } else {
	        return true;
	    }
	}

	function checkRegexp( o, regexp, n ) {
	    if ( !( regexp.test( o.val() ) ) ) {
	        o.addClass( "ui-state-error" );
	        updateTips( n );
	        return false;
	    } else {
	        return true;
	    }
	}

	function updateTips( t ) {
	    var tips = $( "#validateTips" );
	    tips.text( t ).addClass( "ui-state-highlight").css("color", "red");
	    setTimeout(function() {
	        tips.removeClass( "ui-state-highlight", 1500 );
	    }, 500 );
	}
 </script>

