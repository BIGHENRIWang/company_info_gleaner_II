		jQuery(this).find('.fill').each(function() {
				var k = 0, f = jQuery(this), p = f.attr('data-progress-animation'), w = f.width();		
				if (w == 0) {
					p = p.substring(0, p.length - 1);
					var go = function() {
						return k >= p || k>= 100 ?( false ) : ( k += 1, f.css('width', k + '%'), setTimeout(go, 1) )
						};
					go();
				}
			});
		});
	});
