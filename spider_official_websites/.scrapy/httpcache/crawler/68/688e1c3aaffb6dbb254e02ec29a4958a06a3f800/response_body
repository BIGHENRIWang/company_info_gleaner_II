<script>
$(function(){
	$(".FreeLayerBtn").live("click",function(){
		var FreeLayerBox = $(this).parents(".FreeLayerBox");
		FreeLayerBox.toggleClass("current");
	});
	$(".FreeLayerSub").live("click",function(){
		var data=[];
		var FreeLayerBox = $(this).parents(".FreeLayerBox");
		var city = FreeLayerBox.find("select[name=province]").val();
		var keshi = FreeLayerBox.find("select[name=department]").val();
		var tel = FreeLayerBox.find("input[name=phonenumber]").val();
		var xiangxi = $("textarea.xiangxi").val();
		var xingbie = $(".gender input[name=gender]:checked").val();
		if(xiangxi=='请您尽可能详细描述您的症状'){
			xiangxi='';	
		}
		
		$.get('freephone/submitphoneloft/province-'+city+'__department-'+keshi+'__phonenumber-'+tel+'__xiangxi-'+xiangxi+'__xingbie-'+xingbie, function (result)
		{		
			alert(result)
		});

		
		
	});
});
