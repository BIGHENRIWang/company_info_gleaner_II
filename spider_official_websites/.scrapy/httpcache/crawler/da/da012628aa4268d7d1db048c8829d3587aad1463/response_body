function is_wap() {
     var sUserAgent = window.navigator.userAgent;
     var url = "http://www.china-tjftz.gov.cn/html/cntjzymyqn/portal/index/index.htm";
     if (sUserAgent == '') {
         window.location=url;
     }
     else if (sUserAgent.indexOf('untrusted') != -1) 
     {
        window.location=url;
     }
     else if (sUserAgent.indexOf('nokia') != -1) {
        window.location=url;
     }
     else if (sUserAgent.indexOf('sony') != -1) {
        window.location=url;
     }
     else if (sUserAgent.indexOf('ericsson') != -1) 
     {
        window.location=url;
     }
     else if (sUserAgent.indexOf('mot') != -1) 
     {
        window.location=url;
     }
     else if (sUserAgent.indexOf('samsung') != -1) 
     {
        window.location=url;
     }
     else if (sUserAgent.indexOf('sgh') != -1) 
     {
        window.location=url;
     }
     else if (sUserAgent.indexOf('lg') != -1) 
     {
         window.location=url;
     }
     else if (sUserAgent.indexOf('philips') != -1) 
     {
         window.location=url;
     }
     else if (sUserAgent.indexOf('panasonic') != -1) 
     {
         window.location=url;
     }
     else if (sUserAgent.indexOf('alcatel') != -1) 
     {
         window.location=url;
     }
     else if (sUserAgent.indexOf('lenovo') != -1) 
     {
         window.location=url;
     }
     else if (sUserAgent.indexOf('cldc') != -1) 
     {
         window.location=url;
     }
     else if (sUserAgent.indexOf('midp') != -1) 
     {
         window.location=url;
     }
     else if (sUserAgent.indexOf('wap') != -1) 
     {
         window.location=url;
     }
     else if (sUserAgent.indexOf('mobile') != -1) 
     {
         window.location=url;
     }
     else if (sUserAgent.indexOf('Android') != -1) {
         window.location=url;
     }
     else if (sUserAgent.indexOf('iPhone') != -1) 
     {
         window.location=url;
     }
     else if (sUserAgent.indexOf('iPod') != -1) 
     {
         window.location=url;
     }
     else if (sUserAgent.indexOf('iPad') != -1) 
     {
         window.location=url;
     }
     else if (sUserAgent.indexOf('Windows Phone') != -1) 
     {
         window.location=url;
     }
     else if (sUserAgent.indexOf('MQQBrowser') != -1) 
     {
         window.location=url;
     }  
	 else{window.location.href="http://www.china-tjftz.gov.cn/html/cntjzymyqn/portal/index/index.htm";}
 }
