<script type="text/javascript">
      $(function() {
        //首席专家横向移动视频
        (function(){
            var _wrap = jQuery('#expertStyle>ul');
            var _interval = 2000;
            var _moving;
            _wrap.hover(function(){
                clearInterval(_moving);
            },function(){
                _moving=setInterval(function(){
                    var _field=_wrap.find('li:first');
                    var _w=_field.width();
                    _field.animate({marginLeft:-_w+'px'},600,function(){
                        _field.css('marginLeft',0).appendTo(_wrap);
                    });
                },_interval)
            }).trigger('mouseleave');
       })();
    });</script>
