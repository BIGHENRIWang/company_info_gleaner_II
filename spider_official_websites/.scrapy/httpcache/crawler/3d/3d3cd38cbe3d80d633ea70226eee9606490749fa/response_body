	<script language="JavaScript">
		if ($("#search_key input[type=text]").val() == "站内搜索" || $("#search_key input[type=text]").val() == "") {
			$("#search_key input[type=text]").val("站内搜索").css({
				"font-weight" : "normal",
				"color" : "#aaa"
			});
		}
		$("#search_key input[type=text]").click(function() {
			if ($(this).val() == "站内搜索") {
				$(this).val("").css({
					"font-weight" : "bold",
					"color" : "#666"
				});
			}
			$(this).select();
		});

		function check_sear() {
			var _form_key = $("#search_key input[type=text]").val();
			if (_form_key == null || _form_key == "" || _form_key == "站内搜索") {
				alert("请输入关键字!");
				document.form_ind_sear.q.focus();
				document.form_ind_sear.q.select();
				return false;
			}
		}

	</script>

