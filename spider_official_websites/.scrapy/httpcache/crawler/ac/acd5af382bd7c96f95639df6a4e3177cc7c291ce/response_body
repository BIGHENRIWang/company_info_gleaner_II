    <script>
	var oImg=document.getElementById('bannerimg');
	var aImg=oImg.getElementsByTagName('img');
	var oText=document.getElementById('bannertext');
	var aText=oText.getElementsByTagName('dl');
	var oSub=document.getElementById('bannersub');
	var aSub=oSub.getElementsByTagName('a');
	for(var i=0;i<aImg.length;i++){
		aSub[i].index=i;
		aSub[i].onmouseover=function(){
			for(i=0;i<aImg.length;i++){
				$(aImg[i]).fadeOut();
				aText[i].style.display='none';
				aSub[i].className='';
			}
			$(aImg[this.index]).fadeIn();
			aText[this.index].style.display='block';
			aSub[this.index].className='cur';
		}
	}
	</script>


