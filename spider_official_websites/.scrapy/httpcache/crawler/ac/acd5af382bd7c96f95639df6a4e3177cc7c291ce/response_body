<script>
	var rightbox=document.getElementById('right2conbox');
	var rightdl=rightbox.getElementsByTagName('dl');
var g=0;
function rig(g){
	for(r=0;r<rightdl.length;r++){
		if(g==r){
			$(rightbox).animate({left:-g*330+'px'},300)
		}
	}
}
function rigl(){
	g--;
	if(g==-1){g=rightdl.length-1}
	rig(g);
}
function rigr(){
	g++;
	if(g==rightdl.length){g=0}
	rig(g);
}
</script>            
