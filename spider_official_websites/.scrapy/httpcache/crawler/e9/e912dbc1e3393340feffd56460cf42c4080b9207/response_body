<script type="text/javascript">
	function settab(name,cur,n)
{
    for(i=1;i<=n;i++)
	{
		var menu=document.getElementById(name+i);
		var con=document.getElementById("con_"+name+"_"+i);
		con.style.display=(i==cur)?"block":"none";
		var conn=document.getElementById("con_two_"+i);
		conn.style.display=(i==cur)?"block":"none";
		var connn=document.getElementById("red"+i);
		connn.className=(i==cur)?"cRed":""
		menu.className=(i==cur)?"current":""
	}

}

          function settabfos(name,cur,n)
{	
    for(i=1;i<=n;i++)
	{
		var menu=document.getElementById(name+i);
		var con=document.getElementById("con_"+name+"_"+i);
		con.style.display=(i==cur)?"block":"none";
		var connn=document.getElementById("redfos"+i);
		connn.className=(i==cur)?"cRed":""
		menu.className=(i==cur)?"current":""
	}

}


 function settabyysw(name,cur,n)
{	
    for(i=1;i<=n;i++)
	{
		var menu=document.getElementById(name+i);
		var con=document.getElementById("con_"+name+"_"+i);
		con.style.display=(i==cur)?"block":"none";
		var connn=document.getElementById("redfos"+i);
		connn.className=(i==cur)?"cRed":""
		menu.className=(i==cur)?"current":""
	}

}

</script>

