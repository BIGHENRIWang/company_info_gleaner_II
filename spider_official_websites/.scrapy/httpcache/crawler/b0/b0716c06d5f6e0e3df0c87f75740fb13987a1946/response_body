            function setcookie(name,value){
                var Days = 30;
                var exp  = new Date();
                exp.setTime(exp.getTime() + Days*24*60*60*1000);
                document.cookie = name + "="+ escape (value) + ";expires=" + exp.toGMTString();
            }
            function setcookiepath(name,value,path){
                var Days = 30;
                var exp  = new Date();
                exp.setTime(exp.getTime() + Days*24*60*60*1000);
                document.cookie = name + "="+ escape (value) + ";expires=" + exp.toGMTString()+";path="+path;
            }

            function getcookie(name){
                var arr = document.cookie.match(new RegExp("(^| )"+name+"=([^;]*)(;|$)"));
                if(arr != null){
                    return unescape(arr[2]);
                }else{
                    return "";
                }
            }

            function delcookie(name){
                var exp = new Date();
                exp.setTime(exp.getTime() - 1);
                var cval=getCookie(name);
                if(cval!=null) document.cookie= name + "="+cval+";expires="+exp.toGMTString();
            }

            var t=0;
            function ttt(){
                if (t==0)
                    document.aa.F_email.focus();
                var cos=document.cookie.split(";");
                var xos;
                var auth_fc=0;
                var login_auth_count=-1;
                for(var i=0;i<cos.length;i++){
                    if (cos[i].indexOf("LOGIN_AUTH_CODE=")!=-1){
                        xos=cos[i].split("=");
                        login_auth_count=parseInt(xos[1]);

                        break;
                    }
                }
                if (login_auth_count==-1){
                    return;
                }
                if (login_auth_count==0){
                    view_authcode();
                    return;
                }
                for(var i=0;i<cos.length;i++){
                    if (cos[i].indexOf("AUTH_FAIL_COUNT=")!=-1){
                        cos=cos[i].split("=");
                        auth_fc=parseInt(cos[1]);
                        break;
                    }
                }
                if (auth_fc>=login_auth_count){
                    view_authcode();
                }
            }
            function getpos(e) {
                var t=e.offsetTop;
                var l=e.offsetLeft;
                var height=e.offsetHeight;
                while(e=e.offsetParent) {
                    t+=e.offsetTop;
                    l+=e.offsetLeft;
                }
                return [t,l];
            }
            function focusfun(){
                var d=document.getElementById("codetd");
                var c=getpos(document.getElementById("F_code"));
                d.style.top=c[0]-10;
                document.getElementById("codetd").style.display="block";
	
            }
            function onblurfun(){
                setTimeout(function(){document.getElementById("codetd").style.display="none";},200);
            }
            function view_authcode(){
                document.getElementById("auth_code_row").removeAttribute("style");
                document.getElementById("auth_code_td").innerHTML="<input type=text name=F_code id=F_code size=6 class=field onfocus='this.className=&quot;field2&quot;' onblur='this.className=&quot;field&quot;'>";
                document.getElementById("codetd").innerHTML="<nobr><img src=t_img.php id=img_index style='height:32px;width:88px;'><a href=# onclick='update_index()' style='color:#777777'>换一张</a></nobr>";
                document.getElementById("auth_code_row2").style.display="";
            }
            function tt(){
                t=1;
            }
            function  gotohttps()
            {
                var index_url;
   
                index_url = location.href;
                if (index_url.indexOf("https://")){
                    location.href=index_url.replace("http","https");
                }
            }


            function chkCookie()
            {
                var cdm= document.cookie.indexOf("cdomain=");
                var dsel=document.getElementById("domain_sel");
                var keys=document.cookie.split(";");
                var cdmstr="";
                var cumstr="";
                for(var i=0;i<keys.length;i++){
                    var t=keys[i].split("=");
                    var key=t[0].replace(" ","");
                    if (key=="cdomain"){
                        cdmstr=t[1];
                    }
                    if (key=="cusername"){
                        cumstr=t[1];
                    }
                }
                if (cdmstr != "")
                    dsel.value=cdmstr;
                else if (dsel && dsel.options && dsel.options.length > 0)
                    dsel.options[0].selected = true;
                if (cumstr)
                    document.getElementById("F_email").value=cumstr;

                var offset = document.cookie.indexOf("dy_id");
                if ( offset == -1 ) {
                    return;
                }
                var endstr = document.cookie.indexOf(";", offset);
                if ( endstr == -1 ) {
                    endstr = document.cookie.length;
                }
                var dy_email = unescape(document.cookie.substring(offset, endstr));
                var arr_email = dy_email.split("=", 2);
                if (arr_email[1])
                    document.aa.F_email.value = arr_email[1];
                document.aa.F_save_id.checked = true;
            }
            var rsapubkey="-----BEGIN PUBLIC KEY-----MIGfMA0GCSqGSIb3DQEBAQUAA4GNADCBiQKBgQC5FvP6Z/qKeOScNctnEb59A1gvRx5AZJytri9UW3ggf1fYhjZ+I8y0dwh25V45GxfbIxfNwIaYRI0vb1NpUhCUi5gry65hNkVegQh8wT0p8+vAq8JBxEKG98m/ZuVy/+jtLkH7EPtXYxz3LJrULsS8gwb4pJ9HC+DgTbUMTc17KwIDAQAB-----END PUBLIC KEY-----";
            function safe_pass(){
                var rsapass=document.getElementById("F_rsapassword");
                var textpass=document.getElementById("F_password");
                rsapass.value=any_rsa_pass(rsapubkey,"1498557499_"+textpass.value);
                textpass.value="";
            }
            function  setSub()
            {
                var saveuname=document.getElementById("F_save_id");
                if (saveuname && saveuname.checked){
                    setcookie("cusername",document.getElementById("F_email").value);
                }else{
                }
                var sel=document.getElementById("domain_sel");
                if (sel){
                    setcookie("cdomain",sel.value);
                }
                var ssllogin_o = document.getElementById("F_ssllogin");
                if ( !ssllogin_o.checked && false) {
                    safe_pass();
                    return true;;
                }
                ssl_url = "login.php";
                index_url = location.href;
                if ( index_url.indexOf("https://") ){
                    document.getElementById("F_logintype").value = "1";
                }
                proc_pos = index_url.indexOf(":");
                path_pos = index_url.lastIndexOf("/");
                ssl_href = "https" + index_url.substring(proc_pos, path_pos+1) + ssl_url;
                document.aa.action = ssl_href;
                safe_pass();
                document.aa.submit();
            }
            function loginAct() {
                document.getElementById("rlogin").value = (new Date()).getTime().toString();
            }
