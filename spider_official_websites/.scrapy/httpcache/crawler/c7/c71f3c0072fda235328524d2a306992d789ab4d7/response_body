			clearInterval(scrollTimer); 
		},function(){
			scrollTimer = setInterval(function(){ 
				scrollL_R($this);
			},5000); 
		}).trigger("mouseout");
		$(".artists-cont").slidelf({
			"prev":"prev2",
			"next":"next2",
