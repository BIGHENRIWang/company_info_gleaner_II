<script>
	$(function(){
		// 艺术家公盘焦点图切换
//		$(".slide").slidelf({
//			"prev":"next",
//			"next":"prev",
//			"speed":500
//		});
		// 动态获取class为index-list的宽度 = 其下子元素的宽度的之和。
		$(".index-list").width($(".index-list").children("li").length*720);
		$(".artist-list").width($(".artist-list").children("li").length*$(".artist-list").children("li").outerWidth(true));
//		$(".gp-new-list").width($(".gp-new-list").children("li").length*$(".gp-new-list").children("li").width());
		//最新公告,当标题文字字数大于14字，自动换行
		var _lis = $(".jq-anno").find("li");
		var _aS = _lis.find("a");
		var l = 14;
		var _len = _aS.length;
		for(var i= 0; i < _len;i++){
			if(_aS.eq(i).text().length >= l){
				_lis.eq(i).addClass("lh20").removeClass("lh49");
				_aS.eq(i).addClass("br").removeClass("nobr");
			}else{
				_lis.eq(i).addClass("lh49").removeClass("lh20");
				_aS.eq(i).addClass("nobr").removeClass("br");
			}
		}
	});
	
	//艺术家公盘左右滚动效果和上下滚动效果  update at 20150521，START Temporarily！
