jQuery(function(){
	var indicatorTemplate = jQuery.telligent.evolution.template.compile('fragment-7d92546f5f994b78a9264b9dab1c29731121467091_indicatorTemplate');
	jQuery('body').append(indicatorTemplate({}));

	var isPreviewing =  false ,
		previewLink = jQuery('#fragment-7d92546f5f994b78a9264b9dab1c29731121467091_previewLink'),
		previewUrl = '#_cptype=panel\u0026_cpcontexttype=Theme\u0026_cppanelid=a81369a5-aa60-4034-8a25-07df22cd9c42',
		inited = false;

	function open() {
		window.location = previewUrl;
		jQuery.fn.evolutionTip.hide();
		return false;
	}

	function init() {
		if(inited) { return; }
		inited = true;
		previewLink.show()
			.on('mouseenter', function() { $(this).addClass('with-loader'); })
			.on('mouseleave', function(){ $(this).removeClass('with-loader'); })
			.on('glowDelayedMouseEnter', 500, open)
			.on('click', open);
		jQuery.telligent.evolution.messaging.subscribe('themepreview.disabled', function(){
			previewLink.hide();
		});
	}

	if (isPreviewing) { init(); }
	jQuery.telligent.evolution.messaging.subscribe('themepreview.enabled', function(){
		init();
		previewLink.show();
	});
});
