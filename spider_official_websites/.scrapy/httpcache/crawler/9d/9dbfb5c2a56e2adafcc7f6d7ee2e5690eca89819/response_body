		<script>
		jQuery(function(j){
			// no sub-nav on touch
			if(j('body').hasClass('touch'))
				return;

			var container = j('#header-418_container');
			var popup;

			function hideLinksPopup() {
				if(popup) {
					popup.glowPopUpPanel('hide');
				}
			}

			function showLinksPopUp(parent, children) {
				var links = j('<ul></ul>').append(children);
				links.on('mouseleave', function(){
					hideLinksPopup();
				});

				if(popup) {
					hideLinksPopup();
					popup.glowPopUpPanel('empty', '')
						.glowPopUpPanel('append', links)
						.glowPopUpPanel('show', parent);
				} else {
					popup = j('.popupOptions')
						.glowPopUpPanel({
						    cssClass: 'links-popup-panel',
							zIndex: 1500,
							hideOnDocumentClick: true
						})
						.glowPopUpPanel('append', links)
						.glowPopUpPanel('show', parent)
				}
			}

    	container.on('click', '.navigation-list-item a', function(e){

					var elm = j(e.target);
				var popupDiv  = j('.popupOptions');
				var children = elm.data('_link_children');
				if(!children) {
					children = elm.closest('ul').find('ul.children > li');
					
					if(!children && children.length == 0)
					{
					    children = popupDiv.find('ul').children();
					}
					
					elm.data('_link_children', children);
				}
				
			    if(popupDiv.children().length == 0)
			    {
			       var links = j('<ul></ul>').append(children);
			       popupDiv.append(links);
			    }
			   
			    if(e.target.href == "")
			    {
			        popupDiv.toggle();
			    }
			
			});

            $(document).mouseup(function (e)
                {
                    var container = $(".popupOptions");
                    var navigationContainer =  j('#header-418_container .navigation-list-item a');
               
                    if (!container.is(e.target) 
                        && container.has(e.target).length === 0 && !navigationContainer.is(e.target)) 
                    {
                        container.hide();
                    }
                });
			
		});
		</script>
