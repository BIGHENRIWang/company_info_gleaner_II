function show_fq() {
		var v_range=document.invest_search.area.value;
		setLawDisabled();
		if (v_range=="1"){
			document.getElementById("level_type").disabled=false;
		}else{
			document.getElementById("level_type").disabled=true;
		}

		if (v_range=="0"){
			document.getElementById("country").value=0;
			document.getElementById("country").disabled=true;
		}else if (v_range=="3"){
			document.getElementById("country").disabled=true;
		}else{
			document.getElementById("country").disabled=false;
		}
			
		
			
		
			
		
		if (v_range=="" || v_range=="0") {
			document.getElementById("fq").style.display='block';
			return;
		}
		
		var j=fenqu_array[v_range].length;
		document.invest_search.country.options.length=0;
		for (var i=0;i<j;i++){
			if(v_range==1&&i==1){
				continue;
			}
			document.invest_search.country.add(new Option(fenqu_array[v_range][i],i));
		}
			//document.invest_search.country.options[i]=new Option(fenqu_array[v_range][i],i);
		return;
	}
	/**
 * 设置知识产权法律法规库“省市或国家”disabled
 */
function setLawDisabled() {
	var s=document.getElementById("area").value;
	if(s==0)
		document.getElementById("country").disabled=true;
}
function show_fq1(s) {
		var v_range=document.invest_search.area.value;
		var f_range=s;
		var j=fenqu_array[v_range].length;
		document.invest_search.country.options.length=0;
		for (var i=0;i<j;i++) {
			if(v_range==1&&i==1){
				continue;
			}
			document.invest_search.country.add(new Option(fenqu_array[v_range][i],i));
			//document.invest_search.country.options[i]=new Option(fenqu_array[v_range][i],i);
			if(f_range==i) document.invest_search.country.options[i].selected=true;
		}
		return;
	}
	
function searchLaw(){
			var search = document.getElementById("keyword").value;
			var language = document.getElementById("language").value;
			var content_type = document.getElementById("content_type").value;
			var area = document.getElementById("area").value;
			var country = document.getElementById("country").value;
			var level_type = document.getElementById("level_type").value;
			var time_range = document.getElementById("time_range").value;
			//alert(search);
			var url = "/law/list.shtml";
			var flag = document.getElementsByName("radio");
			 if(flag[0].checked){
			 	var url = url +"?title="+ search;
             }else{
             	var url = url +"?content="+ search;
             }
             
             var url = url +"&language="+ language;     
             var url = url +"&content_type="+ content_type;     
             var url = url +"&area="+ area;     
             var url = url +"&country="+ country;     
             var url = url +"&level_type="+ level_type;     
             var url = url +"&time_range="+ time_range;     
			
			window.location.href=url;
		}
		
$(function(){

$("#clearBtn").click(function(){
		$("#keyword").val("");
		$("#subtitle").val("");
		$("#language").val("0");
		$("#area").val("0");
		$("#level_type").val("0");
		$("#time_range").val("0");
		
		$("#keyWordRadio1").prop("checked","checked");
		$("#country").prop("disabled","disabled");
	});
});
	


</script>
