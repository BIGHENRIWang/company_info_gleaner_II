	$(function() {
		$("#article_title").html(title);
                 $("#article_title").css({color:titleColor});
		if(publishTime != ""){
                     publishTime=publishTime.substr(0,10);
                }
                var html ="日期：" + publishTime;
		if(source != ""){
			html += "  来源：" + source;
		}
		$("#article_info").html(html);
		//$("#article_keyword").html("Keywords : "+keyword);
                if($('.artcle img').length>0){
			var len = $('.artcle img').length;
			for(i=0;i<len;i++){
				if(parseInt($('.artcle img').eq(i).css('width'))>730){
					$('.artcle img').eq(i).css('width','700px')
				}
			}
			
		}
	});
